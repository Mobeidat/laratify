{
    "name": "muhsenmaqsudi/laratify",
    "description": "Laratify is a Laravel 7 UI Library with beautifully handcrafted components using TailwindCSS & AlpineJS",
    "license": "MIT",
    "authors": [
        {
            "name": "muhsenmaqsudi",
            "email": "muhsenmaqsudi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "laravel/framework": "^7.0",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Muhsenmaqsudi\\Laratify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muhsenmaqsudi\\Laratify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muhsenmaqsudi\\Laratify\\Providers\\LaratifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
